
@{
    ViewBag.Title = "AddEditCongregation";
}
<link href="~/Content/MyStyle.css" rel="stylesheet" />
<div class="panel panel-primary" style="margin-top:52px;width:500px;margin-left: 260px;">
    <div class="panel-heading">
        <h4 class="panel-title">Congregation</h4>
    </div>

    <div class="panel-body" style="margin-left: 80px;">
        <span class="RequiredField">@Html.Label(Convert.ToString(TempData["LoginError"])) </span>

        <div class="form-group row ">
            <div class=" col-md-2 col-lg-5" style="margin-left: 58px;">
                <label class="required" style=" margin-left: -60px;">Congregation<span>:</span></label>
            </div>

            <div class="col-md-2 col-lg-3" style="width: 221px;margin-top: -2px; margin-left:-48px">
                <input type="text" name="txtCongeration" />

            </div>
        </div>

        <div class="form-group row ">
            <div class=" col-md-2 col-lg-5" style="margin-left: 58px;">
                <label class="required" style=" margin-left: -60px;">Congregation Code<span>:</span></label>
            </div>

            <div class="col-md-2 col-lg-3" style="width: 221px;margin-top: -2px; margin-left:-48px">
                <input type="text" name="txtCongerationcode" />

            </div>
        </div>

        <div class="form-group row col-md-offset-2">


            <div class="col-sm-6 col-md-5 col-lg-3  no-sm-gutter" style="margin-top:15px">

                <input type="submit" id="btnSave" class="btn btn-primary col-xs-6 col-md-6 col-md-offset-7" value="Save" style="width: 90px;" />


            </div>
            <div class="col-sm-6 col-md-5 col-lg-3  no-sm-gutter" style="margin-left: 26px; margin-top: 14px;">
                <input type="button" id="btnLogin" class="btn btn-primary col-xs-6 col-md-6 col-md-offset-7" value="Cancel" style="width: 90px;" />
            </div>
        </div>
     
    </div>

</div>
