
@{
    ViewBag.Title = "AddEditDioces";
}
<div class="panel panel-primary" style="margin-top:52px;width:950px;margin-left: 150px;">
    <div class="panel-heading">
        <h4 class="panel-title">Diocese</h4>
    </div>


    <div class="panel-body" style="margin-left: 80px;">
        <div class="col-sm-12 col-md-6 col-lg-6 clearfix">

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Congregation <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 100px;margin-top: -2px; margin-left:-48px">
                    <select class="form-control" style="width: 200px;margin-top: -2px;">
                        <option>--Select Congregation</option>
                    </select>
                </div>

                <div class="col-sm-5 col-md-5 col-lg-5  no-sm-gutter">
                    <input type="button" id="btnCongregation" class="btn btn-primary btn-xs col-xs-5 col-md-5 col-md-offset-9" value="..." style="width: 40px; margin-top: 4px;" />
                </div>

            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Address1 <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px; margin-top: -2px; margin-left: -48px">
                    <input type="text" class="form-control col-lg-4" />
                </div>
            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Address3 <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px;margin-top: -2px; margin-left:-48px">
                    <input type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">State <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 100px;margin-top: -2px; margin-left:-48px">
                    <select class="form-control" style="width: 200px;margin-top: -2px;">
                        <option>--Select state--</option>
                    </select>
                </div>

                <div class="col-sm-5 col-md-5 col-lg-5  no-sm-gutter">
                    <input type="button" id="btnState" class="btn btn-primary btn-xs col-xs-5 col-md-5 col-md-offset-9" value="..." style="width: 40px; margin-top: 4px;" />
                </div>

            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Phone <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px;margin-top: -2px; margin-left:-48px">
                    <input type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group row col-md-offset-2">

                <div class="col-sm-6 col-md-5 col-lg-3  no-sm-gutter" style="margin-top:15px">

                    <input type="submit" id="btnSave" class="btn btn-primary col-xs-6 col-md-6 col-md-offset-7" value="Save" style="width: 90px;" />


                </div>
                <div class="col-sm-6 col-md-5 col-lg-3  no-sm-gutter" style="margin-left: 26px; margin-top: 14px;">
                    <input type="button" id="btnLogin" class="btn btn-primary col-xs-6 col-md-6 col-md-offset-7" value="Cancel" style="width: 90px;" />
                </div>
            </div>

        </div>
        <div class="col-sm-12 col-md-6 col-lg-6 clearfix">

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Diocese <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px;margin-top: -2px; margin-left:-48px">
                    <input type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Address2 <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px;margin-top: -2px; margin-left:-48px">
                    <input type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">District <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 100px;margin-top: -2px; margin-left:-48px">
                    <select class="form-control" style="width: 200px;margin-top: -2px;">
                        <option>--Select District--</option>
                    </select>
                </div>

                <div class="col-sm-5 col-md-5 col-lg-5  no-sm-gutter">
                    <input type="button" id="btnDistrict" class="btn btn-primary btn-xs col-xs-5 col-md-5 col-md-offset-9" value="..." style="width: 40px; margin-top: 4px;" />
                </div>

            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">PinCode <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px;margin-top: -2px; margin-left:-48px">
                    <input type="text" class="form-control" />
                </div>
            </div>

            <div class="form-group row ">
                <div class=" col-md-2 col-lg-3" style="margin-left: 0px;">
                    <label class="required" style=" margin-left: -60px;">Email <span>:</span></label>
                </div>

                <div class="col-md-2 col-lg-3" style="width: 230px;margin-top: -2px; margin-left:-48px">
                    <input type="text" class="form-control" />
                </div>
            </div>

        </div>

    </div>

</div>